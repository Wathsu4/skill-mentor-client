# Skill Mentor Client Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=5000

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key-here

# Database (if using client-side database)
VITE_DB_URL=your-database-url-here

# Third-party Services
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT=true
VITE_ENABLE_VIDEO_CALLS=true

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# File Upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Session Configuration
VITE_SESSION_TIMEOUT=3600000

# Environment
NODE_ENV=development
